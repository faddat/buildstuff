{
  "builders": [
    {
      "type": "googlecompute",
      "account_file": "/home/faddat/Desktop/account.json",
      "project_id": "hip-hangout-126906",
      "source_image": "debian-8-jessie-v20160329",
      "zone": "asia-east1-c",
      "disk_size": "25",
      "name": "leaf",
      "image_name": "leaf-{{timestamp}}"
    },
    {
  "type": "qemu",
  "name": "klouds-jessie-qemu",
  "ssh_wait_timeout": "15m",
  "format": "qcow2",
  "accelerator": "kvm",
  "net_device": "virtio-net",
  "disk_cache": "none",
  "disk_interface": "virtio",
  "headless": "false",
  "disk_size": "10140",
  "qemuargs": [
    [
      "-m",
      "2048M"
    ],
    [
      "-smp",
      "2"
    ]
  ],
  "iso_url": "http://cdimage.debian.org/debian-cd/current/amd64/iso-cd/debian-8.4.0-amd64-netinst.iso",
  "iso_checksum_url": "http://cdimage.debian.org/cdimage/release/current/amd64/iso-cd/SHA512SUMS",
  "iso_checksum_type": "sha512",
  "http_directory": "http",
      "ssh_username": "faddat",
      "ssh_password": "pacopower",
  "shutdown_command": "echo 'klouds'|sudo -S shutdown -h now",
  "boot_wait": "5s",
  "boot_command": [
    "<esc><wait>",
    "install <wait>",
    "preseed/url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/vanilla-debian-8-jessie-preseed.cfg <wait>",
    "debian-installer=en_US <wait>",
    "auto <wait>",
    "locale=en_US <wait>",
    "kbd-chooser/method=us <wait>",
    "netcfg/get_hostname=leaflet <wait>",
    "netcfg/get_domain=klouds.org <wait>",
    "fb=false <wait>",
    "debconf/frontend=noninteractive <wait>",
    "console-setup/ask_detect=false <wait>",
    "console-keymaps-at/keymap=us <wait>",
    "keyboard-configuration/xkb-keymap=us <wait>",
    "<enter><wait>"
  ]
}
  ],
  "provisioners": [
    {
      "type": "shell",
      "script": "scripts/leaf.sh"
    }
  ],
  "post-processors": ["vagrant"]
}
